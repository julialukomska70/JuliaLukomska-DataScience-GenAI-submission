        "import pandas as pd\n",
        "\n",
        "# Load the dataset\n",
        "df = pd.read_csv('/content/loan_data.csv')\n",
        "\n",
        "# Separate features (X) from the label (Y)\n",
        "X = df.drop('loan_status', axis=1)\n",
        "Y = df['loan_status']\n",
        "\n",
        "print(\"Features (X) shape:\", X.shape)\n",
        "print(\"Label (Y) shape:\", Y.shape)\n",
        "display(X.head())\n",
        "display(Y.head())"
